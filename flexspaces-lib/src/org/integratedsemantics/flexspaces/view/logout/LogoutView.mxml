<?xml version="1.0" encoding="utf-8"?>
<logout:LogoutViewBase 
    xmlns:fx="http://ns.adobe.com/mxml/2009" 
    xmlns:s="library://ns.adobe.com/flex/spark" 
    xmlns:mx="library://ns.adobe.com/flex/mx"
    xmlns:logout="org.integratedsemantics.flexspaces.view.logout.*"
    creationComplete="onCreationComplete(event)" height="100%" styleName="logout" >	
	
    <fx:Metadata>[ResourceBundle("LogoutView")]</fx:Metadata>
  
    <!--fx:Style source="logout.css"/--> 

    <s:HGroup width="100%" height="100%" horizontalAlign="right" verticalAlign="top">

        <mx:Spacer width="5"/>
            
        <mx:LinkButton id="logoutBtn" label="{resourceManager.getString('LogoutView', 'logoutBtn_label' )}" styleName="logoutLink"/>
  
	</s:HGroup>	
	
</logout:LogoutViewBase>
				            